{
	"name": "express-demo",
	"description": "Hello World Node.js Express web application",
	"version": "0.0.1",
	"private": true,
	"dependencies": {
		"express": "4.17.3",
		"lodash": "4.17.21"
	},
	"devDependencies": {
		"mocha": "5.0.0",
		"mocha-junit-reporter": "^1.23.3",
		"nyc": "^15.0.1",
		"supertest": "3.0.0",
		"unit.js": "^2.1.0"
	},
	"scripts": {
		"test": "MOCHA_FILE=reports/test-result.xml mocha --reporter mocha-junit-reporter --recursive tests --exit",
